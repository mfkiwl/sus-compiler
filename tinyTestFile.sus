module replicatee #(T, int NUM_REPLS) {
        interface replicatee: T data -> T[NUM_REPLS] result

        for int i in 0..NUM_REPLS {
                result[i] = data
        } 
}

module use_replicatee {
        replicatee #(NUM_REPLS: 50, T: type bool) repl

        bool[50] repls = repl(true)
}
